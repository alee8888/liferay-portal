<definition component-name="marketplace-plugins-vldap-web">
	<set-up>
		<execute macro="User#firstLoginPG" />
	</set-up>

	<command name="VLDAPWebSmoke" priority="5">
		<property name="web.plugins.includes" value="vldap-web" />

		<var name="appName" value="Virtual LDAP Server" />

		<execute macro="Page#gotoCP">
			<var name="portletName" value="App Manager" />
		</execute>

		<execute macro="AppManager#uninstallAppCP">
			<var name="appName" value="${appName}" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="App Manager" />
		</execute>

		<execute macro="AppManager#viewUninstallAppCP">
			<var name="appName" value="${appName}" />
		</execute>
	</command>
</definition>