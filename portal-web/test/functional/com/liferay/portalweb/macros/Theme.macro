<definition>
	<command name="selectBulletStyle">
		<execute function="Select" locator1="Select#BULLET_STYLE" value1="${bulletStyle}" />
	</command>

	<command name="selectSameLookAndFeel">
		<execute function="Click" locator1="Radio#SAME_LOOK" />
	</command>

	<command name="selectSpecificLookAndFeel">
		<execute function="Click" locator1="Radio#SPECIFIC_LOOK" />
	</command>
</definition>