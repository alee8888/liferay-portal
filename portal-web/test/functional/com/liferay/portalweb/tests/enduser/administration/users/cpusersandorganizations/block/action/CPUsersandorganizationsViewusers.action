<definition>
	<command name="assertClick">
		<case locator-key1="BASIC_SEARCH_BUTTON">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>

		<case locator-key1="DEACTIVATE">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>

		<case locator-key1="DELETE">
			<execute function="AssertClick#assertTextClickAtAndWait" />
		</case>
	</command>

	<command name="isElementPresent">
		<case locator-key1="USER_TABLE_FIRST_NAME">
			<execute function="IsElementPresent#isElementPresentAfterWait" />
		</case>
	</command>
</definition>