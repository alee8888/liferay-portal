<definition component-name="portal-collaboration">
	<property name="portlet.plugins.includes" value="notifications-portlet" />
	<property name="testray.main.component.name" value="Collaboration" />

	<set-up>
		<execute macro="User#firstLoginPG" />
	</set-up>

	<command name="ViewDefaultNotificationsPortletDeliveryOptionsWithContactsPortletDeployed" priority="5">
		<property name="portlet.plugins.includes" value="contacts-portlet,microblogs-portlet,private-messaging-portlet,so-portlet" />

		<execute macro="Notifications#gotoDockbar" />

		<execute macro="Notifications#navigateToViewAllNotifications" />

		<execute macro="NotificationsPortlet#gotoNotificationDelivery" />

		<execute macro="NotificationsPortlet#viewDefaultNotificationDeliveries">
			<var name="contactsPortletDeployed" value="true" />
		</execute>
	</command>
</definition>